{
  "name": "firebaseui-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib": "ng build firebaseui-angular-library && npx copy README.MD dist/firebaseui-angular-library",
    "prerelease": "cd projects/firebaseui-angular-library && npm version prerelease --preid=beta",
    "_prepatch": "cd projects/firebaseui-angular-library && npm version prepatch  --preid=beta",
    "patch": "cd projects/firebaseui-angular-library && npm version patch",
    "_preminor": "cd projects/firebaseui-angular-library && npm version preminor  --preid=beta",
    "minor": "cd projects/firebaseui-angular-library && npm version minor",
    "_premajor": "cd projects/firebaseui-angular-library && npm version premajor  --preid=beta",
    "major": "cd projects/firebaseui-angular-library && npm version major",
    "publish-dist": "npm run build-lib && npm publish dist/firebaseui-angular-library",
    "publish-dist-beta": "npm run build-lib && npm publish dist/firebaseui-angular-library --tag beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.1",
    "@angular/common": "10.2.1",
    "@angular/compiler": "10.2.1",
    "@angular/core": "10.2.1",
    "@angular/fire": "^6.0.4",
    "@angular/forms": "10.2.1",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "10.2.1",
    "@angular/platform-browser-dynamic": "10.2.1",
    "@angular/router": "10.2.1",
    "core-js": "^3.6.5",
    "firebase": "^8.0.0",
    "firebaseui": "^4.7.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "10.2.0",
    "@angular/compiler-cli": "10.2.1",
    "@angular/language-service": "10.2.1",
    "@types/jasmine": "^3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.6",
    "codelyzer": "^6.0.1",
    "copy": "^0.3.2",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "^10.1.2",
    "protractor": "^7.0.0",
    "ts-node": "^9.0.0",
    "tslint": "5.20.1",
    "typescript": "4.0.5"
  }
}
